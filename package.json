{
	"name": "mlut-project",
	"version": "3.2.1",
	"description": "Template project for development with MLUT",
	"scripts": {
		"gulp": "./node_modules/gulp/bin/gulp.js",
		"build": "./node_modules/gulp/bin/gulp.js build",
		"imgmin": "./node_modules/gulp/bin/gulp.js imgmin",
		"mlut-dist": "cp node_modules/mlut/dist src/libs/mlut -r",
		"mlut-get-css": "cp node_modules/mlut/dist/css/mlut.min.css src/libs/",
		"mlut-get-scss": "cp node_modules/mlut/dist/css/mlut.min.css src/libs/mlut.min.scss",
		"mlut-scss": "cat src/sass/mlut.scss > src/sass/style.scss && rm src/sass/mlut.scss",
		"mlut-src": "cp node_modules/mlut/src/core-* src/ -r && cp node_modules/mlut/src/sass src/ -r"
	},
	"author": "mr150",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/mr150/mlut-project.git"
	},
	"devDependencies": {
		"browser-sync": "^2.26.7",
		"del": "^5.1.0",
		"gulp": "^4.0.2",
		"gulp-autoprefixer": "^7.0.0",
		"gulp-clean-css": "^4.2.0",
		"gulp-group-css-media-queries": "^1.2.2",
		"gulp-plumber": "^1.2.1",
		"gulp-imagemin": "^6.1.0",
		"gulp-purgecss": "^1.2.0",
		"gulp-rigger": "^0.5.8",
		"gulp-size": "^3.0.0",
		"gulp-tabify": "0.0.2",
		"gulp-w3c-html-validation": "0.0.1",
		"gulp-uglify": "^3.0.2",
		"gulp-sourcemaps": "^2.6.5",
		"gulp-stylelint": "^9.0.0",
		"gulp-sass": "^4.0.0",
		"gulp-pug": "^4.0.1",
		"gulp-rename": "^1.4.0",
		"imagemin-guetzli": "^2.0.0",
		"imagemin-mozjpeg": "^8.0.0",
		"imagemin-pngquant": "^8.0.0",
		"mlut": "../mlut-lib",
		"stylelint": "^10.1.0",
		"stylelint-order": "^3.1.0"
	}
}
