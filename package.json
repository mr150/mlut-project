{
	"name": "mlut-project",
	"version": "4.1.0",
	"description": "Template project for development with MLUT",
	"scripts": {
		"gulp": "./node_modules/gulp/bin/gulp.js",
		"build": "./node_modules/gulp/bin/gulp.js build",
		"compile": "./node_modules/gulp/bin/gulp.js compile",
		"ftp": "./node_modules/gulp/bin/gulp.js ftp",
		"svg-sprite": "./node_modules/gulp/bin/gulp.js svg-sprite",
		"imgmin": "./node_modules/gulp/bin/gulp.js imgmin",
		"mlut-dist": "cp node_modules/mlut/dist src/libs/mlut -r",
		"mlut-dist-css": "cp node_modules/mlut/dist/css/mlut.min.css src/libs/",
		"mlut-dist-js": "cp node_modules/mlut/dist/js/mlut.min.js src/libs/",
		"mlut-dist-scss": "cp node_modules/mlut/dist/css/mlut.min.css src/libs/mlut.min.scss",
		"mlut-scss": "cat src/sass/mlut.scss > src/sass/style.scss && rm src/sass/mlut.scss",
		"mlut-js": "cat src/js/mlut.js > src/js/script.js && rm src/js/mlut.js",
		"mlut-src": "cp node_modules/mlut/src/sass/* src/sass/ -r && cp node_modules/mlut/src/js/* src/js/ -r"
	},
	"author": "mr150",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/mr150/mlut-project.git"
	},
	"devDependencies": {
		"browser-sync": "^2.26.7",
		"del": "^5.1.0",
		"gulp": "^4.0.2",
		"gulp-autoprefixer": "^7.0.0",
		"gulp-clean-css": "^4.2.0",
		"gulp-group-css-media-queries": "^1.2.2",
		"gulp-plumber": "^1.2.1",
		"gulp-imagemin": "^6.1.0",
		"gulp-purgecss": "^1.2.0",
		"gulp-rigger": "^0.5.8",
		"gulp-size": "^3.0.0",
		"gulp-tabify": "0.0.2",
		"gulp-w3c-html-validation": "0.0.1",
		"gulp-uglify": "^3.0.2",
		"gulp-sourcemaps": "^2.6.5",
		"gulp-stylelint": "^9.0.0",
		"gulp-svgstore": "^7.0.1",
		"gulp-sass": "^4.0.0",
		"gulp-pug": "^4.0.1",
		"gulp-rename": "^1.4.0",
		"imagemin-guetzli": "^2.0.0",
		"imagemin-mozjpeg": "^8.0.0",
		"imagemin-pngquant": "^8.0.0",
		"imagemin-webp": "^5.1.0",
		"mlut": "^3.0.0",
		"stylelint": "^10.1.0",
		"stylelint-order": "^3.1.0",
		"vinyl-ftp": "^0.6.1"
	}
}
